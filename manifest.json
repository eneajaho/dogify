{
    "name": "Dogify",
    "description": "Change every image in website into random dog pictures.",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Hoom Hoom!"
    },
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["dogify.js"],
        "persistent": false
    },
    "icons": { 
        "16": "icon1.png",
        "32": "icon1.png",
        "48": "icon1.png",
        "128": "icon1.png" 
      }

  }


  